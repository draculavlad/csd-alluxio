  "name": "ALLUXIO",
  "label": "ALLUXIO CSD",
  "description": "The alluxio service",
  "version": "1.0",
  "runAs": {
    "user": "root",
    "group": "root"
  },
  "maxInstances": 1,
  "inExpressWizard": true,
  "icon": "images/icon.png",
  "compatibility": {
    "generation": 2,
    "cdhVersion": {
      "min": "4",
      "max": "5"
    }
